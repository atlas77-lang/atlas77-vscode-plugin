{
  "name": "Atlas77",
  "scopeName": "source.atlas77",
  "fileTypes": ["atlas"],
  "patterns": [
    { "include": "#doccomments" },
    { "include": "#comments" },
    { "include": "#strings" },
    { "include": "#numbers" },
    { "include": "#constants" },
    { "include": "#package" },
    { "include": "#imports" },
    { "include": "#keywords" },
    { "include": "#annotations" },
    { "include": "#enums" },
    { "include": "#unions" },
    { "include": "#structs" },
    { "include": "#functions" },
    { "include": "#externFunctions" },
    { "include": "#destructors" },
    { "include": "#paths" },
    { "include": "#primitivetypes" },
    { "include": "#types" },
    { "include": "#fields" }
  ],
  "repository": {
    "doccomments": {
      "name": "comment.line.documentation.atlas77",
      "match": "^\\s*///.*$"
    },
    "comments": {
      "patterns": [
        { "name": "comment.line.double-slash.atlas77", "match": "//.*$" },
        { "name": "comment.block.atlas77", "begin": "/\\*", "end": "\\*/" }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.quoted.double.atlas77",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            { "name": "constant.character.escape.atlas77", "match": "\\\\." }
          ]
        },
        {
          "name": "string.quoted.single.atlas77",
          "begin": "'",
          "end": "'",
          "patterns": [
            { "name": "constant.character.escape.atlas77", "match": "\\\\." }
          ]
        }
      ]
    },
    "numbers": {
      "name": "constant.numeric.atlas77",
      "match": "\\b\\d+(?:\\.\\d+)?(?:_(?:u|i|f)\\d+|_u64|_i64|_f64)?\\b"
    },
    "primitivetypes": {
      "name": "storage.type.atlas77",
      "match": "\\b(int64|int32|int16|int8|uint64|uint32|uint16|uint8|float64|float32|bool|string|unit|char|extern_ptr|usize|isize)\\b"
    },
    "genericContent": {
      "patterns": [
        {
          "name": "meta.generic.constraint.namespaced.atlas77",
          "match": "\\b([A-Za-z_][A-Za-z0-9_]*)\\s*:\\s*([A-Za-z_][A-Za-z0-9_]*)::([A-Za-z_][A-Za-z0-9_]*)",
          "captures": {
            "1": { "name": "support.type.generic.param.atlas77" },
            "2": { "name": "entity.name.namespace.atlas77" },
            "3": { "name": "entity.name.trait.atlas77" }
          }
        },
        {
          "name": "meta.generic.constraint.atlas77",
          "match": "\\b([A-Za-z_][A-Za-z0-9_]*)\\s*:\\s*([A-Za-z_][A-Za-z0-9_]*)",
          "captures": {
            "1": { "name": "support.type.generic.param.atlas77" },
            "2": { "name": "entity.name.trait.atlas77" }
          }
        },
        {
          "name": "meta.type.generic.nested.atlas77",
          "begin": "\\b([A-Z][A-Za-z0-9_]*)\\s*(<)",
          "beginCaptures": {
            "1": { "name": "support.type.atlas77" },
            "2": { "name": "punctuation.definition.generic.begin.atlas77" }
          },
          "end": "(>)",
          "endCaptures": {
            "1": { "name": "punctuation.definition.generic.end.atlas77" }
          },
          "patterns": [
            { "include": "#genericContent" }
          ]
        },
        {
          "name": "support.type.generic.param.atlas77",
          "match": "\\b[A-Z]\\b"
        },
        { "include": "#primitivetypes" },
        {
          "name": "support.type.atlas77",
          "match": "\\b[A-Z][A-Za-z0-9_]*\\b"
        },
        { "name": "punctuation.separator.atlas77", "match": "," },
        { "name": "storage.type.reference.atlas77", "match": "&" }
      ]
    },
    "keywords": {
      "patterns": [
        { "name": "keyword.control.atlas77", "match": "\\b(if|else|while|for|return|break|continue|match|as|where|new|delete|extern|import|package|fun|struct|enum|union|concept|operator)\\b" },
        { "name": "keyword.modifier.atlas77", "match": "\\b(public|private|let|const)\\b" },
        { "name": "keyword.storage.atlas77", "match": "\\b(this|null)\\b" },
        { "name": "keyword.modifier.section.atlas77", "match": "^\\s*(public|private)\\s*:\\s*$" }
      ]
    },
    "annotations": {
      "name": "meta.annotation.atlas77",
      "begin": "#\\[",
      "end": "\\]",
      "patterns": [
        { "name": "entity.name.annotation.atlas77", "match": "[a-zA-Z_][a-zA-Z0-9_]*" }
      ]
    },
    "package": {
      "name": "meta.package.atlas77",
      "begin": "\\b(package)\\b",
      "beginCaptures": { "1": { "name": "keyword.control.atlas77" } },
      "end": ";",
      "patterns": [
        { "name": "entity.name.namespace.atlas77", "match": "[A-Za-z_][A-Za-z0-9_]*" },
        { "name": "punctuation.separator.atlas77", "match": "::" }
      ]
    },
    "imports": {
      "name": "meta.import.atlas77",
      "begin": "\\b(import)\\b",
      "beginCaptures": { "1": { "name": "keyword.control.atlas77" } },
      "end": ";",
      "patterns": [
        { "name": "string.quoted.double.atlas77", "begin": "\"", "end": "\"" },
        { "name": "entity.name.namespace.atlas77", "match": "[A-Za-z_][A-Za-z0-9_]*" },
        { "name": "punctuation.separator.atlas77", "match": "::|/" }
      ]
    },
    "types": {
      "patterns": [
        {
          "name": "meta.type.generic.atlas77",
          "begin": "\\b([A-Z][A-Za-z0-9_]*)\\s*(<)",
          "beginCaptures": {
            "1": { "name": "support.type.atlas77" },
            "2": { "name": "punctuation.definition.generic.begin.atlas77" }
          },
          "end": "(>)",
          "endCaptures": {
            "1": { "name": "punctuation.definition.generic.end.atlas77" }
          },
          "patterns": [
            { "include": "#genericContent" }
          ]
        },
        {
          "name": "storage.type.reference.atlas77",
          "match": "&\\s*(?:const\\s+)?"
        },
        {
          "name": "support.type.atlas77",
          "match": "\\b[A-Z][A-Za-z0-9_]*\\b"
        }
      ]
    },
    "structs": {
      "name": "meta.struct.atlas77",
      "begin": "\\b(?:(public|private)\\s+)?struct\\s+([A-Za-z_][A-Za-z0-9_]*)",
      "beginCaptures": {
        "1": { "name": "keyword.modifier.atlas77" },
        "2": { "name": "entity.name.struct.atlas77" }
      },
      "end": "(?=\\})",
      "patterns": [
        { "include": "#comments" },
        { "include": "#annotations" },
        { "include": "#keywords" },
        {
          "name": "variable.other.member.atlas77",
          "match": "([a-zA-Z_][a-zA-Z0-9_]*)\\s*:"
        }
      ]
    },
    "enums": {
      "name": "meta.enum.atlas77",
      "begin": "\\b(?:(public|private)\\s+)?enum\\s+([A-Za-z_][A-Za-z0-9_]*)",
      "beginCaptures": {
        "1": { "name": "keyword.modifier.atlas77" },
        "2": { "name": "entity.name.enum.atlas77" }
      },
      "end": "(?=\\})",
      "patterns": [
        { "include": "#comments" },
        { "name": "constant.enum.atlas77", "match": "\\b[A-Za-z_][A-Za-z0-9_]*\\b" }
      ]
    },
    "unions": {
      "name": "meta.union.atlas77",
      "begin": "\\b(?:(public|private)\\s+)?union\\s+([A-Za-z_][A-Za-z0-9_]*)",
      "beginCaptures": {
        "1": { "name": "keyword.modifier.atlas77" },
        "2": { "name": "entity.name.union.atlas77" }
      },
      "end": "(?=\\})",
      "patterns": [
        { "include": "#comments" },
        { "include": "#keywords" },
        { "name": "variable.other.member.atlas77", "match": "([a-zA-Z_][a-zA-Z0-9_]*)\\s*:" }
      ]
    },
    "functions": {
      "patterns": [
        {
          "name": "meta.function.atlas77",
          "begin": "\\b(?:(public|private)\\s+)?fun\\s+([A-Za-z_][A-Za-z0-9_]*)\\s*(?=<|\\()",
          "beginCaptures": {
            "1": { "name": "keyword.modifier.atlas77" },
            "2": { "name": "entity.name.function.atlas77" }
          },
          "end": "(?=\\{)",
          "patterns": [
            {
              "name": "meta.function.generic.atlas77",
              "begin": "<",
              "end": ">",
              "patterns": [{ "include": "#genericContent" }]
            },
            { "name": "variable.parameter.atlas77", "match": "([a-zA-Z_][a-zA-Z0-9_]*)(?=\\s*:)" },
            { "name": "keyword.operator.arrow.atlas77", "match": "->" },
            { "include": "#primitivetypes" },
            { "include": "#types" },
            { "name": "storage.type.reference.atlas77", "match": "&" },
            { "name": "punctuation.separator.atlas77", "match": "[,:]" }
          ]
        }
      ]
    },
    "externFunctions": {
      "patterns": [
        {
          "name": "meta.extern.atlas77",
          "begin": "\\b(?:(public|private)\\s+)?extern\\s+([A-Za-z_][A-Za-z0-9_]*)\\s*(?=<|\\()",
          "beginCaptures": {
            "1": { "name": "keyword.modifier.atlas77" },
            "2": { "name": "entity.name.function.atlas77" }
          },
          "end": "(?=;)",
          "patterns": [
            {
              "name": "meta.extern.generic.atlas77",
              "begin": "<",
              "end": ">",
              "patterns": [{ "include": "#genericContent" }]
            },
            { "name": "variable.parameter.atlas77", "match": "([a-zA-Z_][a-zA-Z0-9_]*)(?=\\s*:)" },
            { "name": "keyword.operator.arrow.atlas77", "match": "->" },
            { "include": "#primitivetypes" },
            { "include": "#types" },
            { "name": "storage.type.reference.atlas77", "match": "&" },
            { "name": "punctuation.separator.atlas77", "match": "[,:]" }
          ]
        }
      ]
    },
    "paths": {
      "patterns": [
        {
          "name": "meta.path.atlas77",
          "match": "\\b([A-Za-z_][A-Za-z0-9_]*)\\s*(?=<[^>]+>::)",
          "captures": {
            "1": { "name": "support.type.atlas77" }
          }
        },
        {
          "name": "meta.path.atlas77",
          "match": "::([A-Za-z_][A-Za-z0-9_]*)\\s*(?=\\()",
          "captures": {
            "1": { "name": "entity.name.function.atlas77" }
          }
        },
        {
          "name": "meta.path.atlas77",
          "match": "\\b([A-Za-z_][A-Za-z0-9_]*)::([A-Za-z_][A-Za-z0-9_]*)",
          "captures": {
            "1": { "name": "support.type.atlas77" },
            "2": { "name": "entity.name.function.atlas77" }
          }
        },
        {
          "name": "meta.new.atlas77",
          "match": "\\b(new)\\s+([A-Za-z_][A-Za-z0-9_]*)",
          "captures": {
            "1": { "name": "keyword.control.atlas77" },
            "2": { "name": "support.type.atlas77" }
          }
        }
      ]
    },
    "destructors": {
      "name": "meta.destructor.atlas77",
      "match": "~\\s*([A-Za-z_][A-Za-z0-9_]*)\\s*\\(",
      "captures": { "1": { "name": "entity.name.function.atlas77" } }
    },
    "fields": {
      "patterns": [
        {
          "name": "variable.field.atlas77",
          "match": "this\\.([a-zA-Z_][a-zA-Z0-9_]*)"
        },
        {
          "name": "meta.method.call.atlas77",
          "match": "\\b([a-zA-Z_][a-zA-Z0-9_]*)\\s*\\.\\s*([a-zA-Z_][a-zA-Z0-9_]*)\\s*(?=\\()",
          "captures": {
            "1": { "name": "variable.other.atlas77" },
            "2": { "name": "entity.name.function.atlas77" }
          }
        },
        {
          "name": "meta.field.access.atlas77",
          "match": "\\b([a-zA-Z_][a-zA-Z0-9_]*)\\s*\\.\\s*([a-zA-Z_][a-zA-Z0-9_]*)",
          "captures": {
            "1": { "name": "variable.other.atlas77" },
            "2": { "name": "variable.field.atlas77" }
          }
        },
        {
          "name": "meta.function.call.atlas77",
          "match": "\\b([a-zA-Z_][a-zA-Z0-9_]*)\\s*(?=\\()",
          "captures": {
            "1": { "name": "entity.name.function.atlas77" }
          }
        },
        {
          "name": "variable.other.atlas77",
          "match": "\\b([a-zA-Z_][a-zA-Z0-9_]*)\\b"
        }
      ]
    },
    "constants": {
      "name": "constant.language.atlas77",
      "match": "\\b(true|false)\\b"
    }
  }
}

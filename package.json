{
  "name": "atlas77",
  "displayName": "atlas77",
  "description": "Syntax Highlighting support for the Atlas77 language",
  "repository": {
    "type": "git",
    "url": "https://github.com/atlas77-lang/atlas77-vscode-plugin"
  },
  "icon": "images/atlas77_file_logo.png",
  "version": "0.2.0",
  "publisher": "Gipson62",
  "author": "Gipson62",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "atlas77",
        "aliases": [
          "Atlas77",
          "atlas77",
          "atlas",
          "atlas_77",
          "atlas-77"
        ],
        "extensions": [
          ".atlas"
        ],
        
        "icon": {
          "light": "images/atlas77_file_logo.png",
          "dark": "images/atlas77_file_logo.png"
        },
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "atlas77",
        "scopeName": "source.atlas77",
        "path": "./syntaxes/atlas77.tmLanguage.json"
      }
    ]
    ,
    "themes": [
      {
        "label": "Atlas77 Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/atlas77-color-theme.json"
      }
    ]
  }
}
